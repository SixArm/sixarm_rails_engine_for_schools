<%
  school_bodies||=@school_bodies
%>
<div ="views school_bodies table">
  <table ="data sortable">
    <thead>
      <tr>
        <th><%=t('ID')%></th>
        <th><%=t('XID')%></th>
        <th><%=t('Name')%></th>
      </tr>
    </thead>
    <tbody>
      <% school_bodies.each do |x| %>
        <tr>
          <td><%=x.id%></td>
          <td><%=x.xid%></td>
          <td><%=x.name%></td>
        </tr>
      <%  end %>
    </tbody>
    <tfoot></tfoot>
  </table>
</div>
