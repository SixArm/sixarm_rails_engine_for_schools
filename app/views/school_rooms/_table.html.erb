<%
  school_rooms||=@school_rooms
%>
<div ="views school_rooms table">
  <table ="data sortable">
    <thead>
      <tr>
        <th><%=t('ID')%></th>
        <th><%=t('XID')%></th>
        <th><%=t('Name')%></th>
      </tr>
    </thead>
    <troom>
      <% school_rooms.each do |x| %>
        <tr>
          <td><%=x.id%></td>
          <td><%=x.xid%></td>
          <td><%=x.name%></td>
        </tr>
      <%  end %>
    </troom>
    <tfoot></tfoot>
  </table>
</div>
